{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) 시작",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "프로그램 이름 입력(예: ${workspaceFolder}/a.exe)",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "console": "externalTerminal"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "c:/Users/손승일/Desktop/학교/컴구/프로젝트",
      "program": "c:/Users/손승일/Desktop/학교/컴구/프로젝트/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}